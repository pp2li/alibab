#include <cmath>
#include <iostream>
#include <cstdio>
#include <cstring>
#include <queue>
#include <cstdlib>
#include <set>
#include <stack>
#include <algorithm>

#define MAX 1001
#define lowbit(x) ((x)&(-x))
using namespace std;

int d[MAX][10001];

int main()
{
#ifdef LOCAL
    freopen("in.txt","r",stdin);
#endif // LOCAl
    int q;
    cin>>q;
    while (q--)
    {
        memset(d, 0, sizeof(d));
        string s;
        cin>>s;
        int sum = 0,n = s.size();
        for(int i = 0; i < n; i++)
            sum += s[i] - '0';
        for(int i = 0; i<=9;i++)
            d[1][i] = 1;
        for(int i = 2;i <= n;i++)
        {
            for(int j = 0;j <= i * 9;j++)
            {
                for(int k = 0;k <= 9;k++)
                {
                    if(j-k >= 0)
                        d[i][j] = (d[i][j] + d[i-1][j-k]) % 1000000007;
                }
            // cout<<d[i][j]<<" ";
            }
            // cout<<endl;
        }
        cout<<(d[n][sum]-1) % 1000000007<<endl;
    }
    
    return 0;
}
